function quoteSlides(n) {
  var i;
  var slides = document.getElementsByClassName("myQuotes");
  var quotedots = document.getElementsByClassName("quotedot");
  if (n > slides.length) {slideIndex = 1}    
  if (n < 1) {slideIndex = slides.length}
  for (i = 0; i < slides.length; i++) {
      slides[i].style.display = "none";  
  }
  for (i = 0; i < quotedots.length; i++) {
      quotedots[i].className = quotedots[i].className.replace(" activequote", "");
  }
  slides[slideIndex-1].style.display = "block";  
  quotedots[slideIndex-1].className += " activequote";
}
</script>



  

