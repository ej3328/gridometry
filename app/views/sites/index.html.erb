	function scrollFunction() {
	    if (document.body.scrollTop > 1 || document.documentElement.scrollTop > 1){
	    	$('#nav-bar').addClass('scroll_nav');
	    	$('#nav-bar').removeClass('fixed_nav');
	    	$('#menu').addClass('scroll_nav_links');	
	    	$('#menu').removeClass('fixed_nav_links');	
	    }
	    else {
	    	$('#nav-bar').addClass('fixed_nav');	    
	    	$('#nav-bar').removeClass('scroll_nav');  
	    	$('#menu').addClass('fixed_nav_links');	
	    	$('#menu').removeClass('scroll_nav_links');	
	    }
	}
});


//Scroll page to nav links 
$(document).ready(function(){
  // Add smooth scrolling to all links in navbar + footer link
  $(".menu a").on('click', function(event) {
    // Make sure this.hash has a value before overriding default behavior
    if (this.hash !== "") {
      // Prevent default anchor click behavior
      event.preventDefault();
      // Store hash
      var hash = this.hash;
      // Using jQuery's animate() method to add smooth page scroll
      // The optional number (900) specifies the number of milliseconds it takes to scroll to the specified area
      $('html, body').animate({
        scrollTop: $(hash).offset().top
      }, 900, function(){
   
        // Add hash (#) to URL when done scrolling (default click behavior)
        window.location.hash = hash;
      });
    } // End if
  });
  
});

